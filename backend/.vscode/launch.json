{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/routes/places.js"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: build",
      "request": "launch",
      "command": "npm start",
      "cwd": "${workspaceFolder}"
    }
  ]
}
